
@{
    ViewBag.Title = "thongbao";
    Layout = "~/Views/Shared/_Layoutuser.cshtml";
}
<div class="container" style="background-color:white;">
    <section id="form" style="background-color:white;">
        <div class="container" style="background-color:white;">
            <div class="signup-form">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <p style="font-size:25px;" class="danger">@ViewData["Thong Bao"]</p>
                        
                        <form action="@Url.Action("LoginSignup", "LoginSignup")">
                            <button type="submit" class="btn btn-default">Trở về</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>



